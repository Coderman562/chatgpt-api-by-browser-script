{
    "compilerOptions": {
      "target": "ES2019",
      "module": "CommonJS",
      "rootDir": ".",
      "outDir": "dist",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }